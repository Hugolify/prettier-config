{
  "$schema": "http://json.schemastore.org/prettierrc",
  "overrides": [
    {
      "files": [
        "*.html"
      ],
      "options": {
        "parser": "go-template"
      }
    }
  ],
  "plugins": [
    "prettier-plugin-go-template"
  ],
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none"
}